{
  "name": "gemini-audio-transcriber",
  "version": "1.0.0",
  "type": "module",
  "description": "CLI tool to transcribe and diarize audio using Google Gemini models",
  "main": "dist/index.js",
  "scripts": {
    "start": "node --import ./register-ts-node.mjs src/index.ts",
    "build": "tsc -p tsconfig.json",
    "agent": "tsx src/index.ts --agent",
    "interactive": "tsx src/index.ts --interactive",
    "agent:verbose": "tsx src/index.ts --agent --verbose"
  },
  "dependencies": {
    "@anthropic-ai/claude-code": "^2.0.0",
    "@aws-sdk/client-s3": "^3.624.0",
    "@aws-sdk/credential-providers": "^3.624.0",
    "@google/generative-ai": "^0.11.0",
    "docx": "^9.5.1",
    "dotenv": "^16.4.5",
    "ora": "^9.0.0",
    "pdfkit": "^0.17.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/pdfkit": "^0.17.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.4.5"
  }
}
